<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Util.Hibernate.Model">
	<class name="Sessions" table="SESSIONS">
		<id name="idSession" type="int" column="idSession">
			<generator class="native" />
		</id>
		<property name="cleSession" column="cleSession" type="string" />
		<property name="dateExpiration" column="dateExpiration" type="timestamp" />
	</class>
</hibernate-mapping>