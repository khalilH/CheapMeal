<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Util.Hibernate.Model">
	<class name="Profils" table="PROFILS">
		<id name="idProfil" type="int" column="idProfil">
			<generator class="native" />
		</id>
		<property name="bio" column="bio" type="string" />
	</class>
</hibernate-mapping>